<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:useLevel="false">
            <solid android:width="1dp" android:color="#80AFB1AF" />
            <size android:width="130dp" android:height="25dp" />
            <corners android:radius="17dp" />

        </shape>
    </item>


    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:useLevel="false">
            <solid android:width="1dp" android:color="@color/list_gray" />
            <size android:width="130dp" android:height="25dp" />
            <corners android:radius="17dp" />
        </shape>
    </item>

</selector><!---->